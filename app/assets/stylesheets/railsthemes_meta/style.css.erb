/* #Site Styles
================================================== */
ul.navigation {
  font-weight: bold;
}
ul.navigation li {
  display: inline;
  font-size: 18px;
  margin: 9px 0 0 9px;
}
ul.navigation li a {
  color: #006060;
  text-decoration: none;
  border-bottom: 3px solid #2c2c2c;
}
ul.navigation li a:hover {
  color: #909091;
  border-bottom: 3px solid #404041;
}

#header {
  background: #ededed;
  background: -moz-linear-gradient(bottom, #ededed 0%, #fefefe 100%);
  background: -webkit-gradient(linear, left bottom, left bottom, color-stop(0%, #ededed), color-stop(100%, #fefefe));
  background: -webkit-linear-gradient(bottom, #ededed 0%, #fefefe 100%);
  background: -o-linear-gradient(bottom, #ededed 0%, #fefefe 100%);
  background: -ms-linear-gradient(bottom, #ededed 0%, #fefefe 100%);
  background: linear-gradient(bottom, #ededed 0%, #fefefe 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#fefefe',GradientType=0 );
  border-bottom: 3px solid #129a9a;
  padding: 10px 0;
}
#header #logo a {
  text-decoration: none;
}
#header ul.navigation {
  margin: 20px 0;
  text-align: right;
}

#titlebar {
  background: #009898;
  background: -moz-linear-gradient(top, #009898 0%, #2a4380 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #009898), color-stop(100%, #2a4380));
  background: -webkit-linear-gradient(top, #009898 0%, #2a4380 100%);
  background: -o-linear-gradient(top, #009898 0%, #2a4380 100%);
  background: -ms-linear-gradient(top, #009898 0%, #2a4380 100%);
  background: linear-gradient(top, #009898 0%, #2a4380 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#009898', endColorstr='#2a4380',GradientType=0 );
  color: #fefefe;
}
#titlebar h1, #titlebar h2, #titlebar h3, #titlebar p, #titlebar strong, #titlebar a {
  color: #fefefe;
}
#titlebar p {
  font-size: 18px;
  line-height: 22px;
}
#titlebar a.button {
  color: #2c2c2c;
}
#titlebar .container {
  background: url(<%= image_path "railsthemes_meta/titlebar_background.png" %>) repeat center center;
  padding: 25px 0;
}
#titlebar .container .row, #titlebar .container h2, #titlebar .container h3 {
  margin: 0;
}
#titlebar .container .featured_content {
  min-height: 260px;
}
#titlebar .container .featured_content h1, #titlebar .container .featured_content h2, #titlebar .container .featured_content h3, #titlebar .container .featured_content p, #titlebar .container .featured_content strong, #titlebar .container .featured_content a {
  color: #2c2c2c;
}

#contents {
  margin-top: 20px;
  margin-bottom: 20px;
}

#footer {
  background: #dedede;
  background: -moz-linear-gradient(top, #dedede 0%, #fefefe 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #dedede), color-stop(100%, #fefefe));
  background: -webkit-linear-gradient(top, #dedede 0%, #fefefe 100%);
  background: -o-linear-gradient(top, #dedede 0%, #fefefe 100%);
  background: -ms-linear-gradient(top, #dedede 0%, #fefefe 100%);
  background: linear-gradient(top, #dedede 0%, #fefefe 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#dedede', endColorstr='#fefefe',GradientType=0 );
  border-top: 1px solid #129a9a;
  font-weight: normal;
  padding: 10px 0;
}
#footer ul.navigation li {
  display: block;
  margin-bottom: 0.75em;
}

html, body {
  height: 100%;
}

#main {
  clear: both;
  min-height: 100%;
  height: auto !important;
  height: 100%;
  margin-bottom: -175px;
}
#main #footer-spacer {
  height: 175px;
}

#footer {
  clear: both;
  position: relative;
  height: 175px;
}

/* #Page Styles
================================================== */
ul.grid {
  margin: 0;
}
ul.grid li {
  display: inline;
}

div.feature img {
  float: left;
  margin-right: 10px;
}

div.testimonial {
  background-color: #fafafa;
  border: 1px solid #cdcdcd;
  padding: 1em;
}
div.testimonial blockquote {
  border: none;
  margin: 0;
  padding: 0;
}
div.testimonial p {
  font-size: 125%;
  min-height: 60px;
}
div.testimonial cite {
  float: right;
}
div.testimonial img {
  float: left;
  margin-right: 1em;
  height: 60px;
  -webkit-border-radius: 7px;
  -moz-border-radius: 7px;
  -ms-border-radius: 7px;
  -o-border-radius: 7px;
  border-radius: 7px;
}
div.testimonial span.credit {
  margin-left: 10px;
}

blockquote, q {
  color: #898989;
  font-style: italic;
}

blockquote p, p q {
  margin-bottom: 0.25em;
}
blockquote p:before, blockquote p:after, p q:before, p q:after {
  content: '"';
}

blockquote span.date {
  font-style: normal;
}

h1.eye-catcher, h2.eye-catcher, h3.eye-catcher, h4.eye-catcher, h5.eye-catcher {
  font-style: italic;
  text-align: center;
}

.eye-catcher {
  margin-left: 20px;
  margin-right: 20px;
}

table {
  background-color: #fefefe;
}
table td, table th {
  padding: 0.45em;
  border: 1px solid #ababab;
  border-top: none;
}
table th {
  color: #fefefe;
  font-family: 'Crete Round', serif;
  font-weight: 500;
  font-size: 18px;
  line-height: 22px;
  background: #464646;
  background: -moz-linear-gradient(top, #464646 0%, #252525 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #464646), color-stop(100%, #252525));
  background: -webkit-linear-gradient(top, #464646 0%, #252525 100%);
  background: -o-linear-gradient(top, #464646 0%, #252525 100%);
  background: -ms-linear-gradient(top, #464646 0%, #252525 100%);
  background: linear-gradient(top, #464646 0%, #252525 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#464646', endColorstr='#252525',GradientType=0 );
}
table thead tr:first-child, table tbody:first-child tr:first-child {
  border-color: #ababab;
}
table thead tr:first-child th:first-child, table thead tr:first-child td:first-child, table tbody:first-child tr:first-child th:first-child, table tbody:first-child tr:first-child td:first-child {
  -moz-border-radius-topleft: 5px;
  -webkit-border-top-left-radius: 5px;
  border-top-left-radius: 5px;
  border-left: none;
}
table thead tr:first-child th:last-child, table thead tr:first-child td:last-child, table tbody:first-child tr:first-child th:last-child, table tbody:first-child tr:first-child td:last-child {
  -moz-border-radius-topright: 5px;
  -webkit-border-top-right-radius: 5px;
  border-top-right-radius: 5px;
  border-right: none;
}
table tbody tr:nth-of-type(even) {
  background: #e6e6e6;
}

.featured_content {
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
  padding: 10px;
  border: 2px solid #fafafa;
  background-color: #f5f5f5;
}
.featured_content img {
  text-align: center;
}

table.pricing {
  background-color: #fefefe;
}
table.pricing td, table.pricing th {
  text-align: center;
  padding: 0;
  border: none;
  font-family: 'Crete Round', serif;
  font-size: 18px;
  line-height: 30px;
}
table.pricing th {
  background: transparent;
  filter: none;
  color: #2c2c2c;
}
table.pricing thead th {
  vertical-align: bottom;
}
table.pricing thead th h3 {
  color: #fefefe;
  font-family: Helvetica;
  font-size: 24px;
  line-height: 65px;
  background-color: #006060;
  min-width: 120px;
  margin: 0;
}
table.pricing thead th:nth-of-type(even) h3 {
  background: #009898;
  background: -moz-linear-gradient(top, #009898 0%, #006060 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #009898), color-stop(100%, #006060));
  background: -webkit-linear-gradient(top, #009898 0%, #006060 100%);
  background: -o-linear-gradient(top, #009898 0%, #006060 100%);
  background: -ms-linear-gradient(top, #009898 0%, #006060 100%);
  background: linear-gradient(top, #009898 0%, #006060 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#009898', endColorstr='#006060',GradientType=0 );
}
table.pricing thead th:nth-of-type(odd) h3 {
  background: #009898;
  background: -moz-linear-gradient(top, #009898 0%, #0b4a4c 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #009898), color-stop(100%, #0b4a4c));
  background: -webkit-linear-gradient(top, #009898 0%, #0b4a4c 100%);
  background: -o-linear-gradient(top, #009898 0%, #0b4a4c 100%);
  background: -ms-linear-gradient(top, #009898 0%, #0b4a4c 100%);
  background: linear-gradient(top, #009898 0%, #0b4a4c 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#009898', endColorstr='#0b4a4c',GradientType=0 );
}
table.pricing thead th:first-child h3 {
  font-family: 'Crete Round', serif;
  font-weight: 500;
  font-size: 26px;
  line-height: 53px;
  background: #464646;
  background: -moz-linear-gradient(top, #464646 0%, #252525 100%);
  background: -webkit-gradient(linear, left top, left top, color-stop(0%, #464646), color-stop(100%, #252525));
  background: -webkit-linear-gradient(top, #464646 0%, #252525 100%);
  background: -o-linear-gradient(top, #464646 0%, #252525 100%);
  background: -ms-linear-gradient(top, #464646 0%, #252525 100%);
  background: linear-gradient(top, #464646 0%, #252525 100%);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#464646', endColorstr='#252525',GradientType=0 );
}
table.pricing thead th:nth-of-type(1) h3, table.pricing thead th:nth-of-type(2) h3 {
  -moz-border-radius-topleft: 5px;
  -webkit-border-top-left-radius: 5px;
  border-top-left-radius: 5px;
}
table.pricing thead th:last-of-type h3 {
  -moz-border-radius-topright: 5px;
  -webkit-border-top-right-radius: 5px;
  border-top-right-radius: 5px;
}
table.pricing tbody tr:nth-of-type(even) {
  background: #e6e6e6;
}
table.pricing tbody tr th:first-child {
  padding: 0 0.75em;
  line-height: 53px;
  text-align: left;
  border-right: 1px solid #252525;
}
table.pricing tbody tr td {
  color: #006060;
}
table.pricing tbody tr:nth-of-type(odd) td:nth-of-type(2),
table.pricing tfoot tr th:nth-of-type(3) div {
  background: #f5f5f5;
  border-left: solid 1px #d7d7d7;
  border-right: solid 1px #d7d7d7;
}
table.pricing tfoot tr th:nth-of-type(3) div {
  border-bottom: solid 1px #d7d7d7;
  -moz-border-radius-bottomleft: 3px;
  -webkit-border-bottom-left-radius: 3px;
  border-bottom-left-radius: 3px;
  -moz-border-radius-bottomright: 3px;
  -webkit-border-bottom-right-radius: 3px;
  border-bottom-right-radius: 3px;
}
table.pricing tfoot {
  border-top: 1px solid #252525;
}
table.pricing tfoot th {
  color: #0c6b6d;
  font-size: 30px;
  line-height: 60px;
}
table.pricing tfoot th:first-of-type {
  font-size: 24px;
}
table.pricing tfoot .exp {
  margin: 0 0.5em;
  color: #404041;
  text-align: center;
  font-size: 18px;
  line-height: 18px;
}

