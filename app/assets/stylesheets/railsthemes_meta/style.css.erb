h1, h2, h3, h4, h5, h6, #header {
  font-family: 'Crete Round', serif;
}

body, ul.navigation {
  font-family: 'PT Sans Caption', sans-serif;
}

code {
  font-family: 'Droid Sans Mono', fixed-width, sans-serif;
}

p:last-child {
  margin-bottom: 0;
}
